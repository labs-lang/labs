<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetLatestRuntimePatch>true</TargetLatestRuntimePatch>
  </PropertyGroup>
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.2</TargetFramework>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'Flock' ">
    <StartAction>Project</StartAction>
    <StartArguments>--file ../examples/flock.labs --values birds=10 delta=21 grid=16 k=4 --bound 10 --fair</StartArguments>
    <ExternalConsole>false</ExternalConsole>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'Flock2info' ">
    <StartAction>Project</StartAction>
    <StartArguments>--file ../examples/flock.labs --values birds=3 delta=21 grid=16 --bound 16 --info</StartArguments>
    <ExternalConsole>false</ExternalConsole>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'PhilosophersInfo' ">
    <StartAction>Project</StartAction>
    <StartArguments>--file ../examples/philosophers.labs --bound 2 --values n=3 --info</StartArguments>
    <ExternalConsole>false</ExternalConsole>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'Philosophers' ">
    <StartAction>Project</StartAction>
    <StartArguments>--file ../examples/philosophers.labs --bound 2 --values n=3</StartArguments>
    <ExternalConsole>false</ExternalConsole>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'Formation1d' ">
    <StartAction>Project</StartAction>
    <StartArguments>--file ../examples/formation1d.labs --values n=5 max=30 range=4 --bound 16</StartArguments>
    <ExternalConsole>false</ExternalConsole>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'Formation1dInfo' ">
    <StartAction>Project</StartAction>
    <StartArguments>--file ../examples/formation1d.labs --values n=5 max=30 range=4 --bound 16 --info</StartArguments>
    <ExternalConsole>false</ExternalConsole>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <OtherFlags>--simpleresolution --nocopyfsharpcore --warnon:1182</OtherFlags>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugSymbols>true</DebugSymbols>
    <DefineConstants>RELEASE;NETCOREAPP</DefineConstants>
    <GenerateTailCalls>true</GenerateTailCalls>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'Rescue' ">
    <StartAction>Project</StartAction>
    <StartArguments>--file ../examples/rescue.labs --values n=1 delta=21 grid=16 --bound 16</StartArguments>
    <ExternalConsole>false</ExternalConsole>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Update="Microsoft.NETCore.App" />
  </ItemGroup>
  <ItemGroup>
    <RuntimeHostConfigurationOption Include="System.Globalization.Invariant" Value="true" />
    <Compile Include="Base.fs" />
    <Compile Include="Liquid.fs" />
    <Compile Include="Templates.fs" />
    <Compile Include="ArgParse.fs" />
    <Compile Include="Checks.fs" />
    <Compile Include="Expressions.fs" />
    <Compile Include="Properties.fs" />
    <Compile Include="EncodeInit.fs" />
    <Compile Include="Encode.fs" />
    <Compile Include="Program.fs" />
    <ProjectReference Include="..\LabsCore\LabsCore.fsproj" />
    <ProjectReference Include="..\LabsParser\LabsParser.fsproj" />
    <PackageReference Include="Argu" Version="5.1.0" />
    <PackageReference Include="DotLiquid" Version="2.0.298" />
  </ItemGroup>
</Project>
