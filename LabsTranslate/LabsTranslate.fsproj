<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.1</TargetFramework>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'Flock' ">
    <StartAction>Project</StartAction>
    <StartArguments>--file ../examples/flock.labs --values birds=10 delta=21 grid=16 --bound 10 --fair</StartArguments>
    <ExternalConsole>false</ExternalConsole>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'Flock2' ">
    <StartAction>Project</StartAction>
    <StartArguments>--file ../examples/flock.labs --values birds=3 delta=21 grid=16 --bound 16</StartArguments>
    <ExternalConsole>false</ExternalConsole>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'Voter' ">
    <StartAction>Project</StartAction>
    <StartArguments>--file ../examples/voter.labs --bound 10</StartArguments>
    <ExternalConsole>false</ExternalConsole>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'Flock2sim' ">
    <StartAction>Project</StartAction>
    <StartArguments>--file ../examples/flock2.labs --values birds=3 delta=21 grid=16 --bound 16 --simulation</StartArguments>
    <ExternalConsole>false</ExternalConsole>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'Example2info' ">
    <StartAction>Project</StartAction>
    <StartArguments>--file ../examples/example.labs --bound 12 --fair --info</StartArguments>
    <ExternalConsole>false</ExternalConsole>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'Flock2info' ">
    <StartAction>Project</StartAction>
    <StartArguments>--file ../examples/flock.labs --values birds=3 delta=21 grid=16 --bound 16 --info</StartArguments>
    <ExternalConsole>false</ExternalConsole>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Update="Microsoft.NETCore.App" Version="2.1.0" />
</ItemGroup>
  <ItemGroup>
    <Compile Include="Base.fs" />
    <Compile Include="Liquid.fs" />
    <Compile Include="Templates.fs" />
    <Compile Include="ArgParse.fs" />
    <ProjectReference Include="..\LabsCore\LabsCore.fsproj" />
    <ProjectReference Include="..\LabsParser\LabsParser.fsproj" />
    <PackageReference Include="Argu" Version="4.2.1" />
    <PackageReference Include="DotLiquid" Version="2.0.266" />
    <Compile Include="Checks.fs" />
    <Compile Include="Expressions.fs" />
    <Compile Include="Properties.fs" />
    <Compile Include="Encode.fs" />
    <Compile Include="Program.fs" />
  </ItemGroup>
</Project>